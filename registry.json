{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "i11 registry",
  "homepage": "https://i11-registry.vercel.app/",
  "items": [
    {
      "name": "lib",
      "type": "registry:lib",
      "title": "Library",
      "description": "Reusable code across the project",
      "dependencies": ["tailwind-merge"],
      "files": [
        {
          "path": "src/lib/class-merge.ts",
          "type": "registry:lib"
        },
        {
          "path": "src/lib/create-variants.ts",
          "type": "registry:lib"
        }
      ]
    },
    {
      "name": "types",
      "type": "registry:file",
      "title": "Types",
      "description": "Reusable types across the project",
      "dependencies": [],
      "files": [
        {
          "path": "src/types/base.ts",
          "type": "registry:file",
          "target": "src/types/base.ts"
        },
        {
          "path": "src/types/array.ts",
          "type": "registry:file",
          "target": "src/types/array.ts"
        }
      ]
    },
    {
      "name": "globals",
      "type": "registry:style",
      "title": "Global Styles",
      "description": "Global styles for the application",
      "dependencies": [],
      "files": [
        {
          "path": "registry/new-york/styles/globals.css",
          "type": "registry:style"
        }
      ]
    },
    {
      "name": "button",
      "type": "registry:ui",
      "title": "Button",
      "description": "A basic button component",
      "dependencies": ["@radix-ui/react-slot"],
      "registryDependencies": [
        "https://i11-registry.vercel.app/r/lib.json",
        "https://i11-registry.vercel.app/r/globals.json"
      ],
      "files": [
        {
          "path": "registry/new-york/ui/button.tsx",
          "type": "registry:ui"
        }
      ]
    },
    {
      "name": "use-query-state",
      "type": "registry:hook",
      "title": "Query Strings Hook",
      "description": "Hooks for managing URL query parameters in Next.js",
      "dependencies": ["zod@4.1.12"],
      "registryDependencies": ["https://i11-registry.vercel.app/r/types.json"],
      "files": [
        {
          "path": "registry/hooks/use-query-state.ts",
          "type": "registry:hook"
        }
      ]
    }
  ]
}
